{
  "hosting": {
    "public": "dist",
    "cleanUrls": true,
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      }
    ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
